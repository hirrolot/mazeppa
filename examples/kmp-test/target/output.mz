main(s) := f0(s);

f0(s) :=
    match s {
    Cons(s', ss) -> f1(s', ss),
    Nil() -> F()
    };

f1(s, ss) :=
    match =(97u8, s) {
    F() -> f0(ss),
    T() ->
    match ss {
    Cons(s', ss') -> f2(s', ss'),
    Nil() -> F()
    }
    };

f2(s, ss) :=
    match =(97u8, s) {
    F() -> f1(s, ss),
    T() ->
    match ss {
    Cons(s', ss') ->
    match =(98u8, s') {
    F() -> f2(s', ss'),
    T() -> T()
    },
    Nil() -> F()
    }
    };
