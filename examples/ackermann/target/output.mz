main(m, n) := f0(m, n);

f0(m, n) :=
    match =(m, 0u32) {
    F() ->
    match =(n, 0u32) {
    F() -> f1(f1(-(n, 1u32), m), -(m, 1u32)),
    T() -> f2(-(m, 1u32))
    },
    T() -> +(n, 1u32)
    };

f1(v0, m) := f0(m, v0);

f2(v0) :=
    match =(v0, 0u32) {
    F() -> f1(f2(-(v0, 1u32)), -(v0, 1u32)),
    T() -> 2u32
    };
