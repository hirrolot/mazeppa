main(m, n) := f0(m, n);

f0(m, n) :=
    match =(m, 0u32) {
    F() -> f2(m, n),
    T() -> +(n, 1u32)
    };

f1(v0, m) := f0(m, v0);

f2(m, n) :=
    match =(n, 0u32) {
    F() -> f1(f1(-(n, 1u32), m), -(m, 1u32)),
    T() ->
    let v0 := -(m, 1u32);
    match =(v0, 0u32) {
    F() -> f1(f2(v0, 0u32), -(v0, 1u32)),
    T() -> 2u32
    }
    };
